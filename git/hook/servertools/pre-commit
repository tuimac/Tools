#!/bin/bash

function cleanUpLogs(){
    local logpath='src/backend/logs/info_django.log'
    rm $logpath
    touch $logpath
}

function cleanUpTemplate(){
    local dirpath='src/backend/templates'
    rm -rf $dirpath/*
    touch $dirpath/.gitkeep
}

function main(){
    cleanUpLogs
    git add .
}

main
